{
    "name": "@packages/services",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "src/index.d.ts",
    "exports": {
        ".": {
            "types": "./src/index.d.ts",
            "default": "./dist/index.js"
        },
        "./amplify/*": {
            "types": "./src/amplify/*.d.ts",
            "default": "./dist/amplify/*.js"
        }
    },
    "scripts": {
        "build": "tsc --build",
        "tsc": "tsc --noEmit",
        "lint": "eslint -c eslint.config.js .",
        "test": "echo \"(placeholder)\""
    },
    "dependencies": {
        "@packages/domain": "workspace:*",
        "@packages/types": "workspace:*"
    },
    "peerDependencies": {
        "aws-amplify": ">=6",
        "@aws-amplify/ui-react": ">=6",
        "next": ">=15",
        "react": ">=18",
        "react-dom": ">=18"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    }
}
