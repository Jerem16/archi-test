{
    "name": "@packages/services",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./amplify": {
            "types": "./dist/amplify/index.d.ts",
            "default": "./dist/amplify/index.js"
        },
        "./amplify/*": {
            "types": "./dist/amplify/*.d.ts",
            "default": "./dist/amplify/*.js"
        }
    },
    "typesVersions": {
        "*": {
            "amplify": [
                "src/amplify/index.ts"
            ],
            "amplify/*": [
                "src/amplify/*"
            ],
            "*": [
                "src/*"
            ]
        }
    },
    "scripts": {
        "build": "tsc --build",
        "tsc": "tsc --noEmit",
        "lint": "eslint -c eslint.config.js .",
        "test": "echo \"(placeholder)\""
    },
    "dependencies": {
        "@packages/domain": "workspace:*",
        "@packages/types": "workspace:*"
    },
    "peerDependencies": {
        "aws-amplify": ">=6"
    }
}
